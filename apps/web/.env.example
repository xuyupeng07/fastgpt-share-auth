# JWT配置
# 请将此密钥替换为您自己的安全密钥
JWT_SECRET=fastgpt-ultra-secure-jwt-secret-key-2024-1234567890
JWT_ISSUER=fastgpt-auth
JWT_AUDIENCE=fastgpt-share
JWT_EXPIRES_IN=2h

# MongoDB连接配置
# 请替换为您的MongoDB连接字符串
MONGODB_URI=mongodb://username:password@host:port/database?directConnection=true&authSource=admin

# API配置
NEXT_PUBLIC_API_URL=http://localhost:3000

# 环境配置
NODE_ENV=development

# 可选配置
# CORS_ORIGIN=http://localhost:3000
# SESSION_TIMEOUT=3600
# MAX_LOGIN_ATTEMPTS=5